    }
    [annotation-data="scalebar"]
    {
        priority: 1;
        icon-image : "white.png";
        icon-color: #fff;
        icon-size: [8pt * @kipawa-dpi-factor, 13 * @kipawa-dpi-factor];
        icon-position-offset: [ -$scalebar_size + $offset_x, 8pt * @kipawa-dpi-factor + $offset_y];
        icon-anchor-position: [1, 1];
    }
    [annotation-data="scalebar"]
    {
        priority: 1;
        icon-image : "white.png";
        icon-color: #fff;
        icon-size: [8pt * @kipawa-dpi-factor, 13 * @kipawa-dpi-factor];
        icon-position-offset: [ $offset_x, 8pt * @kipawa-dpi-factor + $offset_y];
        icon-anchor-position: [-1, 1];
    }
    [annotation-data="scalebar"]
    {
        priority: 2;
        icon-image : "white.png";
        icon-color: #142843;
        icon-size: [$scalebar_size - 4pt * @kipawa-dpi-factor, 4pt * @kipawa-dpi-factor];
        icon-position-offset : [-2pt * @kipawa-dpi-factor + $offset_x, 2pt * @kipawa-dpi-factor + $offset_y];
        icon-anchor-position: [-1, 1];
    }
    [annotation-data="scalebar"]
    {
        priority: 2;
        icon-image : "white.png";
        icon-color: #142843;
        icon-size: [4pt * @kipawa-dpi-factor, 13 * @kipawa-dpi-factor];
        icon-position-offset: [ -$scalebar_size + 2pt* @kipawa-dpi-factor + $offset_x, 6pt * @kipawa-dpi-factor + $offset_y];
        icon-anchor-position: [1, 1];
    }
    [annotation-data="scalebar"]
    {
        priority: 2;
        icon-image : "white.png";
        icon-color: #142843;
        icon-size: [4pt * @kipawa-dpi-factor, 13 * @kipawa-dpi-factor];
        icon-position-offset: [ -2pt* @kipawa-dpi-factor + $offset_x, 6pt * @kipawa-dpi-factor + $offset_y];
        icon-anchor-position: [-1, 1];
    }
    [annotation-data="scalebar"]
    {
